{
	"name": "@m-social/eslint-config-react",
	"version": "1.0.0-next.5",
	"description": "Shared ESLint config for React used by M-Social projects",
	"keywords": ["m-social", "eslint", "eslint-config", "react"],
	"author": {
		"name": "M-Social",
		"url": "https://msocialproduction.com"
	},
	"contributors": [
		{
			"name": "Artem Pitikin",
			"email": "a.pitikin@msocialproduction.com"
		}
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/m-social/frontend-configs.git",
		"directory": "configs/eslint/react"
	},
	"license": "MIT",
	"files": ["dist", "src"],
	"type": "module",
	"types": "./dist/index.d.ts",
	"module": "./dist/index.js",
	"main": "./dist/index.cjs",
	"exports": {
		".": {
			"@m-social/frontend-configs": "./src/index.ts",
			"types": {
				"import": "./dist/index.d.ts",
				"require": "./dist/index.d.cts"
			},
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "tsdown"
	},
	"dependencies": {
		"@eslint-react/eslint-plugin": "^2.0.6",
		"@m-social/eslint-config-typescript": "workspace:*",
		"@stylistic/eslint-plugin": "^5.4.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react-hooks": "^7.0.0",
		"globals": "catalog:eslint"
	},
	"peerDependencies": {
		"eslint": "catalog:eslint"
	},
	"publishConfig": {
		"access": "public"
	}
}
