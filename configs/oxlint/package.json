{
	"name": "@m-social/oxlint-config",
	"version": "0.4.0",
	"description": "Shared oxlint configs for projects used at M-Social",
	"keywords": ["m-social", "oxlint", "config", "oxlint-config"],
	"author": {
		"name": "M-Social",
		"url": "https://msocialproduction.com"
	},
	"contributors": [
		{
			"name": "Artem Pitikin",
			"email": "a.pitikin@msocialproduction.com"
		}
	],
	"scripts": {
		"build": "tsdown"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/m-social/frontend-configs.git",
		"directory": "configs/oxlint"
	},
	"type": "module",
	"exports": {
		"./typescript": {
			"types": "./dist/typescript.d.ts",
			"import": "./dist/typescript.js"
		},
		"./react": {
			"types": "./dist/react.d.ts",
			"import": "./dist/react.js"
		},
		"./next": {
			"types": "./dist/next.d.ts",
			"import": "./dist/next.js"
		},
		"./eslint/react": {
			"types": "./dist/eslint/react.d.ts",
			"import": "./dist/eslint/react.js"
		}
	},
	"files": ["src", "dist"],
	"devDependencies": {
		"eslint": "catalog:eslint",
		"oxlint": "catalog:oxlint",
		"oxlint-tsgolint": "catalog:oxlint",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"eslint": "catalog:eslint",
		"oxlint": "catalog:oxlint",
		"oxlint-tsgolint": "catalog:oxlint"
	},
	"peerDependenciesMeta": {
		"eslint": {
			"optional": true
		}
	},
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	}
}
